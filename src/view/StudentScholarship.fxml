<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StudentScholarshipController">
   <children>
      <HBox>
         <children>
            <TextField fx:id="filterField" promptText="Search Filter..." />
         </children>
      </HBox>
      <TableView fx:id="table" prefHeight="474.0" prefWidth="882.0">
        <columns>
          <TableColumn fx:id="idCol" text="ID" />
          <TableColumn fx:id="nameCol" text="Name" />
          <TableColumn fx:id="donorCol" text="Donor" />
            <TableColumn fx:id="deadlineCol" text="Deadline" />
            <TableColumn fx:id="amtCol" text="Amount" />
            <TableColumn fx:id="numCol" text="Number" />
            <TableColumn fx:id="facCol" text="Faculty" />
            <TableColumn fx:id="deptCol" text="Department" />
            <TableColumn fx:id="typeCol" text="Type" />
            <TableColumn fx:id="gpaCol" text="GPA" />
            <TableColumn fx:id="yearCol" text="Year" />
        </columns>
      </TableView>
      <Button fx:id="startApplicationButton" mnemonicParsing="false" onAction="#handleStartApplicationButtonAction" text="Start Application">
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
